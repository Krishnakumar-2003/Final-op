{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "npm: dev",
		"type": "npm",
		"script": "dev",
		"group": {
		  "kind": "build",
		  "isDefault": true
		},
		"presentation": {
		  "reveal": "always",
		  "panel": "shared"
		},
		"problemMatcher": {
		  "owner": "javascript",
		  "fileLocation": [
			"relative",
			"${workspaceFolder}"
		  ],
		  "pattern": {
			"regexp": "^(.*)$",
			"file": 1,
			"location": 2,
			"message": 3
		  }
		},
		"isBackground": true,
	  },
	]
  }
  